   "source": [
    "import cv2 as cv\n",
    "img = cv.VideoCapture(0)\n",
    "\n",
    "\n",
    "while img.isOpened():\n",
    "    _,frame = img.read()\n",
    "    key = cv.waitKey(0)\n",
    "    cv.imshow('frame',frame)\n",
    "    if key == 27:\n",
    "        break\n",
    "img.release()\n",
    "cv.destroyAllWindows()\n",
    "    \n"
   ]
